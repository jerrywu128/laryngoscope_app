    <application
        android:name=".Application.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/honestmedical"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/FullScreenTheme"
        android:requestLegacyExternalStorage="true"
        android:usesCleartextTraffic="true"
        >
        <activity
            android:name=".View.Activity.LogoActivity"
            android:launchMode="singleTask"
            android:screenOrientation="sensorPortrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Activity.LaunchActivity"
            android:launchMode="singleTask"
            android:screenOrientation="sensorPortrait"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".View.Activity.PreviewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_preview"
            android:launchMode="singleTask"
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity
            android:name=".View.Activity.LocalVideoWallActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_local_video_wall"
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity
            android:name=".View.Activity.LocalPhotoWallActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_local_photo_wall"
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity
            android:name=".View.Activity.LocalVideoPbActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_pb_local_video"
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity
            android:name=".View.Activity.MultiPbActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label=""
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity
            android:name=".View.Activity.VideoPbActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_video_pb"
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity
            android:name=".View.Activity.PhotoPbActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_photo_pb"
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity
            android:name=".View.Activity.LocalPhotoPbActivity"
            android:label="@string/title_activity_pb_local_photo"
            android:theme="@style/FullScreenTheme.NoActionBar"/>
        <activity android:name=".View.Activity.WifiApActivity"/>
        <activity android:name=".View.Activity.LoginGoogleActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN"/> -->
            <!-- <category android:name="android.intent.category.LAUNCHER"/> -->
            <!-- </intent-filter> -->
        </activity>
        <activity android:name=".View.Activity.BaseActivity">
        </activity>
        <activity
            android:name=".View.Activity.LicenseAgreementActivity"
            android:launchMode="singleTask"
            android:screenOrientation="sensorPortrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/text_license">
        </activity>
        <activity
            android:name=".View.Activity.LaunchHelpActivity"
            android:launchMode="singleTask"
            android:screenOrientation="sensorPortrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/help">
        </activity>
        <activity
            android:name=".View.Activity.ConnectCMActivity"
            android:launchMode="singleTask"
            android:screenOrientation="sensorPortrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/wifiqrcode_connect">
        </activity>
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.icatch.sbcapp.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_provider_path" />
        </provider>
        <service android:name=".Function.MediaCaptureService"
            android:foregroundServiceType="mediaProjection">
        </service>
        <service android:name=".Function.MediaRecordService"
            android:foregroundServiceType="mediaProjection">
        </service>
